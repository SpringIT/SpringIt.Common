    <add key="masstransit.rabbitmq.username" value="guest" />
    <add key="masstransit.rabbitmq.password" value="guest" />
    <add key="masstransit.rabbitmq.endpoint" value="SpringIt.ServiceBus.SimpleInjector.Endpoint" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
  </startup>
  <runtime>
